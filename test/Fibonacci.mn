Space

Comet fibonacci(planet n) -> planet {

    planet a = 0;
    planet b = 1;
    planet c;

    if (n < 0) {
      print("Estas demente? No me vengas con tu negatividad");
      return (-1);
    } elseif (n == 0) {
      return a; 
    }

    orbit i around range(2,n) { 
      c = a + b; 
      a = b; 
      b = c; 
    } 
    return b; 
}


Space

Comet par(planet n) -> planet {
	if (n == 0) {
		return 1
	}
	elseif (n == 1){
		return 0
	} else {
	    planet r = impar(n-1);
		return r
	}
}

Comet impar(planet n) -> planet {
	if (n == 0) {
		return 0
	}
	elseif (n == 1){
		return 1
	} else {
	    planet r = par(n-1);
		return r
	}
}

print("Introduzca numerito: ");
(planet -> planet) Comet g = par;
planet r = g(terraform(read()));
if (r == 0) {
		print("Es impar.")
	} else {
		print("Es par.")
	}

